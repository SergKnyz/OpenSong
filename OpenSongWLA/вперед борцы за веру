  <title>Вперед борцы за веру</title>
  <author></author>
  <copyright></copyright>
  <hymn_number></hymn_number>
  <presentation></presentation>
  <ccli></ccli>
  <capo print="false"></capo>
  <key>F</key>
  <aka></aka>
